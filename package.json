{
  "name": "fermtempctrl",
  "version": "0.1.1",
  "private": true,
  "config": {
    "webpackPort": 4000
  },
  "scripts": {
    "dev": "NODE_ENV=development nodemon --exec node server/dev.js --watch server -e js,json,html",
    "babel": " babel server -D -d build",
    "build": "rm -rf ./build; npm run babel && npm run bundle",
    "bundle": "NODE_ENV=production webpack",
    "bundle-dev": "NODE_ENV=development WEBPACK_PORT=$npm_package_config_webpackPort webpack-dev-server --hot --port=$npm_package_config_webpackPort",
    "start": "NODE_ENV=production node build/index.js",
    "lint": "eslint server/ client/",
    "heroku-postbuild": "npm run build"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "dependencies": {
    "babel-cli": "6.6.5",
    "babel-core": "6.7.2",
    "babel-loader": "6.2.4",
    "babel-polyfill": "6.7.2",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-register": "6.7.2",
    "basic-auth": "1.0.3",
    "body-parser": "1.15.0",
    "compression": "1.6.1",
    "express": "4.13.4",
    "highcharts": "4.2.3",
    "isomorphic-fetch": "2.2.1",
    "jsx-loader": "0.13.2",
    "knex": "0.10.0",
    "lodash": "4.6.1",
    "material-ui": "0.15.0-alpha.2",
    "moment": "2.12.0",
    "particle-api-js": "5.2.3",
    "pg": "4.5.1",
    "react": "0.14.7",
    "react-dom": "0.14.7",
    "react-highcharts": "8.0.3",
    "react-redux": "4.4.1",
    "react-router": "2.0.1",
    "react-tap-event-plugin": "0.2.2",
    "redux": "3.3.1",
    "redux-thunk": "2.0.1",
    "socket.io": "1.4.5",
    "socket.io-client": "1.4.5",
    "webpack": "1.12.14"
  },
  "devDependencies": {
    "eslint": "2.4.0",
    "eslint-config-airbnb": "6.1.0",
    "eslint-plugin-react": "4.2.3",
    "http-proxy": "1.13.2",
    "morgan": "1.7.0",
    "nodemon": "1.9.1",
    "react-hot-loader": "1.3.0",
    "sqlite3": "3.1.1",
    "webpack-dev-server": "1.14.1"
  }
}
