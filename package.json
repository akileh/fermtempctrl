{
  "name": "fermtempctrl",
  "version": "0.1.2",
  "private": true,
  "config": {
    "webpackPort": 4000
  },
  "scripts": {
    "dev": "concurrently \"npm run start-dev\" \"npm run bundle-dev\"",
    "babel": " babel server -D -d build",
    "build": "rm -rf ./build; npm run babel && npm run bundle",
    "bundle": "NODE_ENV=production webpack",
    "bundle-dev": "NODE_ENV=development WEBPACK_PORT=$npm_package_config_webpackPort webpack-dev-server --hot --port=$npm_package_config_webpackPort",
    "start": "NODE_ENV=production node build/index.js",
    "start-dev": "NODE_ENV=development nodemon --exec node server/dev.js --watch server -e js,json,html",
    "lint": "eslint server/ client/",
    "heroku-postbuild": "npm run build",
    "particle-login": "particle cloud login",
    "particle-logout": "particle cloud logout",
    "particle-flash": "particle flash $DEVICE ./particle"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "dependencies": {
    "babel-cli": "6.8.0",
    "babel-core": "6.8.0",
    "babel-loader": "6.2.4",
    "babel-polyfill": "6.8.0",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-register": "6.8.0",
    "basic-auth": "1.0.4",
    "body-parser": "1.15.1",
    "compression": "1.6.2",
    "concurrently": "^2.0.0",
    "express": "4.13.4",
    "highcharts": "4.2.5",
    "isomorphic-fetch": "2.2.1",
    "knex": "^0.11.3",
    "lodash": "4.12.0",
    "material-ui": "0.15.0",
    "moment": "2.13.0",
    "particle-api-js": "5.2.7",
    "particle-cli": "^1.11.0",
    "pg": "4.5.5",
    "react": "15.0.2",
    "react-dom": "15.0.2",
    "react-highcharts": "8.3.2",
    "react-redux": "4.4.5",
    "react-router": "2.4.0",
    "react-tap-event-plugin": "1.0.0",
    "redux": "3.5.2",
    "redux-thunk": "2.1.0",
    "socket.io": "1.4.6",
    "socket.io-client": "1.4.6",
    "webpack": "1.13.0"
  },
  "devDependencies": {
    "eslint": "2.10.1",
    "eslint-config-airbnb-base": "^3.0.1",
    "eslint-plugin-import": "^1.6.1",
    "eslint-plugin-react": "5.1.1",
    "http-proxy": "1.13.3",
    "jsx-loader": "0.13.2",
    "morgan": "1.7.0",
    "nodemon": "1.9.2",
    "react-hot-loader": "1.3.0",
    "sqlite3": "3.1.4",
    "webpack-dev-server": "1.14.1"
  }
}
